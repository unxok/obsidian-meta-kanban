{
	"compilerOptions": {
		"jsx": "react",
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"]
		},
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "ESNext",
		"target": "ES6",
		"allowJs": true,
		// "noImplicitAny": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"lib": ["DOM", "ES5", "ES6", "ES7"]
	},
	"include": ["**/*.ts", "src/main.tsx"]
}
